<div class="container">
<h1>Signin</h1>
<%= form_for :user, url: sessions_path do |f|%>
<label>Name</label><br>
<%= f.text_field :name, class: "form-control" %><br>

<label>Password:</label>
<%= f.password_field :password, class: "form-control" %><br>

<%= f.submit "Sign In", :class  => "btn btn-primary"%>

<% end %>
</div>