<div class="container">
	<div class="jumbotron text-center">
		<h1>The Dogs of Doggy Rate</h1>
		<p>Click on a dog to see their profile and rate them</p>
	</div>
</div>

<div class="container text-center">
	<% @dogs.each do |dog| %>
	<%= link_to "#{dog.name} the #{dog.breed}", dog_path(dog) %><br>
	<div class="media">
 		<%= image_tag(dog.picture.image.url, class: 'media-object small-image') %>
	</div>	
	<% end %>
</div>